<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-default.xml" />
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<constant name="struts.objectFactory" value="spring" />
	<package name="struts2" extends="struts-default" namespace="/">

		<action name="userac" class="userAction">
			<result name="success">/success.jsp</result>
			<result name="logoff">/logoffsuc.jsp</result>
			<result name="error">/error.jsp</result>
			<result name="input">/index.jsp</result>
		</action>
		<action name="regist" class="registAction">
			<result name="success">/index.jsp</result>
			<result name="error">/registerror.jsp</result>
			<result name="input">/regist.jsp</result>
		</action>
		<action name="alter" class="alterAction">
			<result name="success">/success.jsp</result>
			<result name="error">/error.jsp</result>
			<result name="input">/alterpass.jsp</result>
		</action>
		<action name="forgetpass" class="forgetpassAction">
			<result name="success">/sendsuc.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="logoff" class="logoffAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="alteraccount" class="alteraccountAction">
			<result name="success">/success.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="search" class="searchAction">
			<result name="success">/Result.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
	</package>

</struts>
